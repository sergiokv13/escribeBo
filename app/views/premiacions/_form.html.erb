<%= bootstrap_form_for @premiacion do |f| %>
  <div class="form-group">
    <%= f.text_field :title ,class:"form-control"%>
  </div>
  <div class="form-group">
    <%= f.date_select :date_of ,class:"form-control"%>
  </div>
  <div class="form-group" hidden="true">
    <%= f.text_field :user_id, value: @user.id%>
  </div>
  <div class="actions">
    <%= f.submit "Premiar", class: "btn btn-success" %>
  </div>
<% end %>

