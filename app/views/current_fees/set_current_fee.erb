<center>
  <h1>Actualizar cuadro de tasas.</h1>
  <% if !@current_fee.nil? %>
  <strong>Actual</strong>
  <br>
  <img src="<%=@current_fee.image%>">
  <% end %>
  <%= form_tag "/update_current_fee", method: :post, multipart: true do  %>


    <div class="form-group">
      <%= file_field_tag :image %>
    </div>

    <div class="actions">
      <%= submit_tag "Actualizar", class: "btn btn-success" %>
    </div>


  <% end %>
</center>